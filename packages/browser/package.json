{
  "name": "@datadog/openfeature-browser",
  "version": "0.1.0",
  "description": "Browser-specific bindings for OpenFeature (wraps @datadog/openfeature-core)",
  "license": "MIT",
  "homepage": "https://github.com/DataDog/openfeature-javascript-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/DataDog/openfeature-javascript-client.git",
    "directory": "packages/browser"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm"
  },
  "dependencies": {
    "@datadog/openfeature-core": "0.1.0-alpha.1"
  },
  "peerDependencies": {}
}