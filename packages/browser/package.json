{
  "name": "@datadog/openfeature-browser",
  "version": "0.1.0-alpha.2",
  "description": "Browser-specific bindings for OpenFeature (wraps @datadog/openfeature-core)",
  "license": "MIT",
  "homepage": "https://github.com/DataDog/openfeature-js-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/DataDog/openfeature-js-client.git",
    "directory": "packages/browser"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepack": "npm run build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@datadog/flagging-core": "^0.1.0-alpha.1",
    "@openfeature/core": "^1.8.1",
    "@openfeature/web-sdk": "^1.5.0"
  },
  "devDependencies": {
    "@datadog/browser-core": "^6.10.0",
    "@types/jest": "^29.4.0",
    "ts-jest": "^29.4.0"
  },
  "peerDependencies": {
    "@datadog/browser-core": ">=6.0.0"
  }
}
